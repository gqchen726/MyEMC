package com.briup.bean;

public enum FileNameEnums {
	CLIENT_FLAG_PATH("client_flag_path", "src/main/resources/flag.txt"),
	CLIENT_DATA_PATH("client_data_path", "src/main/resources/data.txt"),
	CLIENT_SEND_DATA_PATH("client_send_data_path", "src/main/resources/send-data-backup.txt"),
	SERVER_ACCEPT_DATA_PATH("server_accept_data_path","src/main/resources/server-data-backup.txt");

	private String fileName;
	private String path;

	private FileNameEnums(String fileName, String path) {
		this.fileName = fileName;
		this.path = path;
	}

	public String getFileName() {
		return fileName;
	}

	public void setFileName(String fileName) {
		this.fileName = fileName;
	}

	public String getPath() {
		return path;
	}

	public void setPath(String path) {
		this.path = path;
	}

}
